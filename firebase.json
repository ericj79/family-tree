{
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "hosting": {
        "target": "tree",
        "public": "dist\\family-tree",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [{
            "source": "*",
            "headers": [{
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            }]
        }],
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }]
    }

}
